{
  "name": "clean-react",
  "version": "1.6.3",
  "main": "src/main/index.tsx",
  "repository": "https://github.com/leonfoliveira/clean-react.git",
  "author": "Leonardo Oliveira <leonfoliveira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start": "node index.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "cypress install && start-server-and-test dev http-get://localhost:8080 test:cypress:run",
    "update": "yarn-check -u -s"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cypress": "^6.1.0",
    "cypress-webpack-preprocessor-v5": "^5.0.0-alpha.1",
    "eslint": "7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "favicons-webpack-plugin": "^5.0.0-alpha.3",
    "full-icu": "^1.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "history": "^5.0.0",
