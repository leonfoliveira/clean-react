    "jest": "^26.6.3",
    "jest-localstorage-mock": "2.4.6",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "start-server-and-test": "^1.11.6",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "5.11.0",
    "webpack-cli": "^4.2.0",
